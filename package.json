{
  "name": "sapbot",
  "version": "2.0.0",
  "description": "open source node.js/socket.io chat app",
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.2",
    "chart.js": "^2.7.2",
    "compression": "1.7.1",
    "config": "^1.30.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^6.0.0",
    "express": "4.16.2",
    "jquery": "^3.3.1",
    "libphonenumber-js": "^1.2.19",
    "method-override": "2.3.10",
    "mime-types": "^2.1.19",
    "mississippi": "^3.0.0",
    "natural": "^0.6.1",
    "popper.js": "^1.14.3",
    "quagga": "^0.12.1",
    "serve-static": "1.13.1",
    "sms_ru": "0.0.1",
    "socket.io": "2.0.4",
    "socket.io-stream": "^0.9.1",
    "socketio-file-upload": "^0.6.1",
    "superagent": "^3.8.3",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "build": "browserify client/index.js -d -p [minifyify --no-map] > public/scripts/dist.js",
    "build-dev": "browserify client/index.js -o public/scripts/dist.js --debug",
    "watch": "watchify client/index.js -o public/scripts/dist.js --debug",
    "start": "yarn run build & node index",
    "start-dev": "yarn run build-dev & node index"
  },
  "license": "MIT",
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "browserify-shim": "^3.8.14",
    "cssify": "^1.0.3",
    "minifyify": "^7.3.5",
    "watchify": "^3.11.0"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "cssify"
    ]
  },
  "browserify-shim": {
    "jquery": "$"
  }
}
